{
    "name": "vocweb/oauth2-tiki",
    "description": "OAuth2 for Tiki",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "league/oauth2-client": "^2.6"
    },
    "license": "MIT",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "tiki"
    ],
    "autoload": {
        "psr-4": {
            "Tiki\\Oauth2\\Client\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tiki\\OAuth2\\Client\\Providers\\TikiAuthProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Tien Chu",
            "email": "chutien@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    }
}
